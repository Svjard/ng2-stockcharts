{
  "name": "ng2-stockcharts",
  "version": "0.0.0",
  "description": "Highly customizable Angular 2 ng2-stockcharts",
  "main": "index.js",
  "scripts": {
    "build:system": "node node_modules/typescript/bin/tsc -sourcemap -p tsconfig.system.json",
    "build:cjs": "node node_modules/typescript/bin/tsc -sourcemap",
    "build": "npm run sass && npm run build:cjs && npm run build:system",
    "demo:watch": "./node_modules/.bin/webpack --progress --colors --watch",
    "demo:dist": "./node_modules/.bin/webpack --progress --colors --dist",
    "lint": "npm run sass && npm run lint:src && npm run lint:docs",
    "lint:src": "npm run sass && ./node_modules/.bin/tslint --config tslint.json --exclude 'node_modules/**/*' src/**/*.ts",
    "lint:docs": "./node_modules/.bin/tslint --config tslint.json --exclude 'node_modules/**/*' demo/**/*.ts",
    "postinstall": "./node_modules/.bin/typings install",
    "version:major": "npm version major",
    "version:premajor": "npm version premajor",
    "version:minor": "npm version minor",
    "version:preminor": "npm version preminor",
    "version:patch": "npm version patch",
    "version:prepatch": "npm version prepatch",
    "version:prerelease": "npm version prerelease",
    "sass": "./node_modules/typescript-sass/bin/ts-sass",
    "test": "./node_modules/.bin/karma start karma.conf.js",
    "validate": "npm ls"
  },
  "files": [
    "dist",
    "index.js",
    "index.d.ts"
  ],
  "keywords": [
    "angular2",
    "stockcharts",
    "d3"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Svjard/ng2-stockcharts"
  },
  "author": "Marc Fisher",
  "devDependencies": {
    "bulma": "^0.0.14",
    "codelyzer": "0.0.19",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.35.0",
    "highlight.js": "^9.1.0",
    "html-loader": "^0.4.3",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.26",
    "karma-teamcity-reporter": "^2.0.0",
    "karma-webpack": "^1.7.0",
    "node-sass": "^3.6.0",
    "phantomjs-prebuilt": "^2.1.7",
    "precommit-hook": "latest",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "0.1.2",
    "remap-istanbul": "^0.6.4",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.7.2",
    "tslint": "^3.7.0-dev.2",
    "typescript": "^1.8.10",
    "typescript-sass": "git+https://github.com/Svjard/typescript-sass.git#master",
    "typings": "^1.0.4",
    "webpack": "^1.12.9"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "core-js": "^2.4.0",
    "d3": "^3.5.17",
    "es6-module-loader": "^0.17.8",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.26",
    "zone.js": "^0.6.12"
  },
  "pre-commit": [
    "lint",
    "validate",
    "test"
  ]
}